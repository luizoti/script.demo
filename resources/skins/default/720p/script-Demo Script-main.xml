<window id="129">
    <defaultcontrol always="false">10</defaultcontrol>
    <coordinates>
        <system>1</system>
        <posx>100</posx>
        <posy>50</posy>
    </coordinates>
    <controls>
        <control type="image">
            <description>Background</description>
            <posx>0</posx>
            <posy>0</posy>            
            <width>900</width>
            <height>500</height>
            <texture>black.png</texture>
            <ondown>10</ondown>
            <onup>10</onup>
            <onleft>10</onleft>
            <onright>10</onright>
        </control>
        <control type="button" id="10">
            <description>Update Library</description>
            <textcolor>white</textcolor>
            <posx>740</posx>
            <posy>200</posy>
            <width>140</width>
            <height>60</height>
            <textoffsetx>12</textoffsetx>
            <align>center</align>
            <aligny>center</aligny>
            <onclick></onclick>
            <label>Update</label>
            <texturenofocus>keyboard-btn-backspace.png</texturenofocus>
            <texturefocus>keyboard-btn-backspace-focus.png</texturefocus>
            <onup>30</onup>
            <ondown>20</ondown>
        </control>
        <control type="button" id="20">
            <description>Clear Library</description>
            <textcolor>white</textcolor>
            <posx>740</posx>
            <posy>300</posy>
            <width>140</width>
            <height>60</height>
            <textoffsetx>12</textoffsetx>
            <align>center</align>
            <aligny>center</aligny>
            <label>Clear</label>
            <onclick>Skin.ToggleSetting(disable_label)</onclick>
            <texturenofocus>keyboard-btn-backspace.png</texturenofocus>
            <texturefocus>keyboard-btn-backspace-focus.png</texturefocus>
            <onup>10</onup>
            <ondown>30</ondown>
        </control>
        <control type="button" id="30">
            <description>Exit Button</description>
            <onclick></onclick>
            <textcolor>white</textcolor>
            <label>Exit</label>
            <posx>740</posx>
            <posy>420</posy>
            <width>140</width>
            <height>60</height>
            <textoffsetx>12</textoffsetx>
            <align>center</align>
            <aligny>center</aligny>
            <texturenofocus>keyboard-btn-backspace.png</texturenofocus>
            <texturefocus>keyboard-btn-backspace-focus.png</texturefocus>
            <onup>20</onup>
            <ondown>10</ondown>
        </control>
        <control type="label" id="40">
                <description>My First label</description>
                <left>80</left>
                <top>60</top>
                <width>250</width>
                <visible>!Skin.HasSetting(disable_label)</visible>
                <align>center</align>
                <aligny>center</aligny>
                <scroll>false</scroll>
                <label>"Teste"</label>
                <angle>30</angle>
                <haspath>false</haspath>
                <font>font14</font>
                <textcolor>FFB2D4F5</textcolor>
                <shadowcolor>ff000000</shadowcolor>
                <wrapmultiline>false</wrapmultiline>
                <scrollspeed>50</scrollspeed>
                <scrollsuffix> - </scrollsuffix>
        </control>
    </controls>
</window>